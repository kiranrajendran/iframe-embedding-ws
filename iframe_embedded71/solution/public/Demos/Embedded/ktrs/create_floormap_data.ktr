        <jsScript_script>var Calendar = Packages.java.util.Calendar;
var Locale = Packages.java.util.Locale;
var SimpleDateFormat = Packages.java.text.SimpleDateFormat;

var year_for_week = (week_of_year == 1 &amp;&amp; month==12)?year+1:year;
var quarter = Math.floor((month-1) / 3) +1;

var cal = Calendar.getInstance();
cal.set(year, month-1, day);
var d = cal.getTime()

// generate labels for months and weeks of day in german and english
var day_of_week_short_label_ger = new SimpleDateFormat("E",Locale.GERMANY).format(d);
var day_of_week_long_label_ger = new SimpleDateFormat("EEEEE",Locale.GERMANY).format(d);
var month_short_label_ger = new SimpleDateFormat("MMM",Locale.GERMANY).format(d);
var month_long_label_ger = new SimpleDateFormat("MMMMM",Locale.GERMANY).format(d);

var day_of_week_short_label_eng = new SimpleDateFormat("E",Locale.US).format(d);
var day_of_week_long_label_eng = new SimpleDateFormat("EEEEE",Locale.US).format(d);
var month_short_label_eng = new SimpleDateFormat("MMM",Locale.US).format(d);
var month_long_label_eng = new SimpleDateFormat("MMMMM",Locale.US).format(d);

// generate amount of days in current month and quarter

var days_in_month = cal.getActualMaximum(Calendar.DAY_OF_MONTH);
var days_in_year = cal.getActualMaximum(Calendar.DAY_OF_YEAR);

var days_in_quarter;

switch(quarter){

	case 1:
		days_in_quarter = (days_in_year == 366)? 31+29+31: 31+28+31; // jan, feb, mar
		break;
	case 2:
		days_in_quarter = 30+31+30;  // apr, may, jun
		break;
	case 3:
		days_in_quarter = 31+31+30;	 // jul, aug, sep
		break;
	case 4:
		days_in_quarter = 31+30+31;	 // oct, nov, dec
		break

}

// gnerate some flags
var is_leap_year = (days_in_year == 366)?1:0;
var is_weekend = (day_of_week == Calendar.SUNDAY || day_of_week == Calendar.SATURDAY)?1:0;

// germany uses ISO8601, using it to calculate week_of_month_iso8601
var isoCal = Calendar.getInstance(Locale.GERMANY);
isoCal.set(year, month-1, day);
var week_of_month_iso8601 = isoCal.get(Calendar.WEEK_OF_MONTH);

// calculate is_leap_year and days_in_year for the week years
cal.set(Calendar.YEAR, year_for_week);
var days_in_year_for_week = cal.getActualMaximum(Calendar.DAY_OF_YEAR);
var is_leap_year_for_week = (days_in_year_for_week==366)?1:0;
cal.set(Calendar.YEAR, year_for_week_iso8601);
var days_in_year_for_week_iso8601 = cal.getActualMaximum(Calendar.DAY_OF_YEAR);
var is_leap_year_for_week_iso8601 = (days_in_year_for_week_iso8601==366)?1:0;


